{
  "name": "@app/shared",
  "version": "0.0.1",
  "description": "Shared utilities and scripts for app template",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "~22.17.1",
    "pnpm": "~10.14.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
