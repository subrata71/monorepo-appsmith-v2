  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.17.1",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.0.0",
    "openapi-typescript": "^7.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.0"
  }
}
